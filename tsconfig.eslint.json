{
  "extends": "./tsconfig",
  "compilerOptions": {
    "noEmit": true
  },
  "include": [".eslintrc.js"],
  "trailing-comma": [
    true,
    {
      "singleline": "never",
      "multiline": {
        "objects": "never",
        "arrays": "always",
        "functions": "never",
        "typeLiterals": "ignore"
      }
    }
  ]
}
